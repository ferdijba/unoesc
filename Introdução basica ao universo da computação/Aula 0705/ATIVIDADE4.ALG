Algoritmo "Atividade4"

Var

   contador: inteiro
   tabuada: inteiro
Inicio

   escreva("Digite o número para tirar a tabuada:")
   leia(tabuada)
   contador <-1
   enquanto (contador < 11) faca
      escreval(tabuada," X ",contador," = ",tabuada * contador)
      contador <- contador +1
   fimenquanto


Fimalgoritmo