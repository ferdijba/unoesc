Algoritmo "exercicio6"
Var
   numero: inteiro


Inicio
   escreva("Digite o numero:")
   leia(numero)

   se(numero < 0) entao
      escreva("Numero é negativo")
   senao
      se (numero > 0) entao
         escreva("Numero é positivo")
      senao
         escreva("Numero igual a 0")
      fimse
   fimse



Fimalgoritmo